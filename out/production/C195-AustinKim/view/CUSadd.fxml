<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="controller.CUSaddController">
   <children>
      <Label layoutX="30.0" layoutY="30.0" text="Customer ID" />
      <Label fx:id="customeridText" layoutX="175.0" layoutY="30.0" prefHeight="17.0" prefWidth="149.0" text="Insert Text" />
      <Label layoutX="30.0" layoutY="70.0" text="Customer Name" />
      <TextField fx:id="customernameText" layoutX="175.0" layoutY="66.0" />
      <Label layoutX="30.0" layoutY="110.0" text="Address" />
      <TextField fx:id="addressText" layoutX="175.0" layoutY="106.0" />
      <Label layoutX="30.0" layoutY="150.0" text="Postal Code" />
      <TextField fx:id="postalcodeText" layoutX="175.0" layoutY="146.0" />
      <Label layoutX="30.0" layoutY="190.0" text="Phone Number" />
      <TextField fx:id="phonenumberText" layoutX="175.0" layoutY="186.0" />
      <Label layoutX="30.0" layoutY="230.0" text="Country" />
      <ComboBox fx:id="countryCBText" layoutX="175.0" layoutY="226.0" onAction="#countrySelected" prefWidth="150.0" />
      <Label layoutX="30.0" layoutY="270.0" text="First-Level Division" />
      <ComboBox fx:id="firstleveldivisionCBText" layoutX="175.0" layoutY="266.0" prefWidth="150.0" />
      <Button layoutX="282.0" layoutY="311.0" mnemonicParsing="false" onAction="#clickSubmitButton" text="Submit" />
      <Button layoutX="214.0" layoutY="311.0" mnemonicParsing="false" onAction="#clickCancelButton" text="Cancel" />
   </children>
</AnchorPane>
